{
  "name": "@fex-to/provider-icons",
  "version": "3.1.9",
  "author": "dejurin",
  "license": "MIT",
  "description": "Financial provider icons: banks, exchanges, central banks. Available in SVG, React components, and PNG",
  "publishConfig": {
    "access": "restricted"
  },
  "workspaces": [
    "packages/*",
    "test/*"
  ],
  "scripts": {
    "rename": "node ./utils/rename-icon.mjs",
    "delete": "node ./utils/delete-icon.mjs",
    "generate:preview": "node ./.build/generate-preview.mjs",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "build": "npm run optimize && npm run update && npm run build:svgs && npm run build:icons && npm run build:packages && npm run preview && npm run changelog-image && npm run zip && npm run update:readme && npm run generate:preview",
    "optimize": "node ./.build/optimize.mjs",
    "update": "npm run update:icons-version",
    "update:icons-version": "node ./.build/update-icons-version.mjs",
    "update:readme": "node ./.build/update-readme.mjs",
    "import": "npm run build:clean && node ./.build/import.mjs && npm run optimize",
    "changelog": "node ./.build/changelog.mjs",
    "changelog-commit": "git add . && node ./.build/changelog-commit.mjs",
    "changelog-image": "node ./.build/changelog-image.mjs",
    "build:packages": "npm run build:react && npm run build:png && npm run build:webp",
    "build:clean": "rm -rf ./icons/*.svg",
    "build:icons": "npm run build --workspace=@fex-to/provider-icons",
    "build:react": "npm run build --workspace=@fex-to/provider-icons-react",
    "build:png": "npm run build --workspace=@fex-to/provider-icons-png",
    "build:webp": "npm run build --workspace=@fex-to/provider-icons-webp",
    "build:svgs": "npm run build:copy && node ./.build/build-svgs.mjs",
    "build:copy": "rm -rf ./icons && mkdir ./icons && cp ./src/_icons/* ./icons/",
    "preview": "npm run preview-icons",
    "preview-icons": "node ./.build/preview-icons.mjs",
    "zip": "node ./.build/zip-files.mjs",
    "release": "git pull && release-it --verbose",
    "changelog-release": "node ./.build/changelog-release.mjs"
  },
  "dependencies": {
    "@atomico/rollup-plugin-sizes": "^1.1.4",
    "@babel/cli": "^7.28.3",
    "@babel/core": "7.28.4",
    "@babel/parser": "7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@release-it-plugins/workspaces": "^5.0.3",
    "@rollup/plugin-babel": "6.1.0",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@svgr/babel-plugin-replace-jsx-attribute-value": "8.0.0",
    "@svgr/core": "8.1.0",
    "@vue/babel-plugin-jsx": "^2.0.1",
    "adm-zip": "^0.5.16",
    "cheerio": "^1.1.2",
    "csv-parser": "^3.2.0",
    "esbuild": "^0.25.10",
    "fs-extra": "^11.3.2",
    "glob": "11.0.3",
    "html-minifier": "^4.0.0",
    "lodash.template": "4.5.0",
    "minimist": "1.2.8",
    "parse-svg-path": "^0.1.2",
    "prettier": "^3.6.2",
    "release-it": "19.0.5",
    "rollup": "4.52.4",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-filesize": "10.0.0",
    "rollup-plugin-license": "^3.6.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-rename": "^1.0.1",
    "rollup-plugin-visualizer": "^6.0.4",
    "svg-outline-stroke": "1.3.1",
    "svgo": "^4.0.0",
    "svgpath": "^2.6.0",
    "svgson": "^5.3.1"
  },
  "release-it": {
    "npm": false,
    "plugins": {
      "@release-it-plugins/workspaces": {
        "workspaces": [
          "packages/*"
        ],
        "publish": true,
        "additionalManifests": {
          "versionUpdates": [
            "package.json",
            "packages/*/package.json"
          ]
        }
      }
    },
    "hooks": {
      "before:init": "git add src/_icons && node ./.build/changelog-release.mjs > RELEASE_NOTES.md && npm run changelog-image",
      "after:bump": "npm run build --latest-version ${latestVersion} --new-version ${version} --verbose",
      "after:release": "echo Successfully released ${name} v${version} to ${repo.repository}.",
      "before:git:init": "npm install"
    },
    "git": {
      "addUntrackedFiles": true,
      "changelog": "npm run changelog --silent --latest-version ${latestTag}",
      "tagName": "v${version}",
      "_requireBranch": "master",
      "requireCommits": true,
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true,
      "releaseNotes": "cat RELEASE_NOTES.md"
    }
  },
  "packageManager": "npm@11.6.2",
  "devDependencies": {
    "@release-it-plugins/workspaces": "^5.0.3",
    "turbo": "^2.5.8"
  }
}